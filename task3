
namespace Consoleap
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(Kok());
        }
        static int Kok(int num = 45)
        {
            int i = 0;
            while (i * i < num)
            {
                i++;
            }
            return i - 1;

        }
    }
}
         
